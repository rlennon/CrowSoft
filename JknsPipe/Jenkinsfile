pipeline {
	agent any
	stages {
		stage('Checkout Code'){
			steps {
		  	checkout scm				
			}	
		}
		stage ('Build MVC'){
			steps {
				sh 'echo MSBuild'
			}
		}

		Stage ('Run Unit Test') {
			steps (
				sh	'echo UnitTest'
			)
		}
    stage('Run Selenium Test') {
	  	stepss {
		  	sh 'echo SeleniumTest'			
		  	}
		}	
	}
}

