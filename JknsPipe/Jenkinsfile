pipeline {
  agent any
  stages {
    stage('GitHub Pull') {
	  	steps {
		  	sh 'echo "*************************GitHub Pull*************************"'
				script {
				git branch: '*/CSoftDevBranch', poll: false, url: 'git@github.com:rlennon/CrowSoft.git'					
				}
			}	
	  }
  	stage('Selenium Testing') {
  		steps {
			  sh 'echo "*************************Selenium Testing*************************"'
 		  }	
	  }
				stage('Build/Zip Artifacts') {
	  	steps {
		  	sh 'echo "*************************Build/Zip Artifacts*************************"'
			
		  }	
	  }
  	stage('Artifactory Load') {
  		steps {
			  sh 'echo "*************************Artifactory Load*************************"'
 		  }	
	  }
  }
}
